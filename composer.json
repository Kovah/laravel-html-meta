{
  "name": "kovah/laravel-html-meta",
  "description": "A Laravel package to parse meta information from an URL.",
  "keywords": [
    "html",
    "meta",
    "parser",
    "laravel"
  ],
  "homepage": "https://github.com/kovah/laravel-html-meta",
  "license": "MIT",
  "authors": [
    {
      "name": "Kevin Woblick",
      "email": "mail@kovah.de",
      "homepage": "https://kovah.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-iconv": "*",
    "guzzlehttp/guzzle": "^7.2",
    "illuminate/http": "^9.0 | ^10.0 | ^11.0 | ^12.0",
    "illuminate/support": "^9.0 | ^10.0 | ^11.0 | ^12.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.4.2",
    "orchestra/testbench": "^7.0 | ^v8.0 | ^9.0 | ^10.0",
    "phpunit/phpunit": "^9.0 | ^10.0"
  },
  "autoload": {
    "psr-4": {
      "Kovah\\HtmlMeta\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kovah\\HtmlMeta\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kovah\\HtmlMeta\\HtmlMetaServiceProvider"
      ],
      "aliases": {
        "HtmlMeta": "Kovah\\HtmlMeta\\Facades\\HtmlMeta"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/kovah"
    }
  ]
}
